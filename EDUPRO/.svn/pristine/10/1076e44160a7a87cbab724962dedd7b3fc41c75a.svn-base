package com.dexter.bradawl.dto;

public class School implements java.io.Serializable
{
	private static final long serialVersionUID = 1L;
	
	private int sch_id;
	private String sch_nm;
	private String sch_motto;
	private int staff_id;
	private int addr_id;
	private String year_founded;
	private String rc_no;
	
	private int op_type_id;
	private String op_type_nm;
	
	private Address address;
	
	public School()
	{}

	public Address getAddress()
	{
		if(address == null)
		{
			address = new Address();
		}
		return address;
	}

	public void setAddress(Address address)
	{
		this.address = address;
	}
	
	public int getSch_id()
	{
		return sch_id;
	}

	public void setSch_id(int schId)
	{
		sch_id = schId;
	}

	public String getSch_nm()
	{
		return sch_nm;
	}

	public void setSch_nm(String schNm)
	{
		sch_nm = schNm;
	}

	public String getSch_motto()
	{
		return sch_motto;
	}

	public void setSch_motto(String schMotto)
	{
		sch_motto = schMotto;
	}

	public int getStaff_id()
	{
		return staff_id;
	}

	public void setStaff_id(int staffId)
	{
		staff_id = staffId;
	}

	public int getAddr_id()
	{
		return addr_id;
	}

	public void setAddr_id(int addrId)
	{
		addr_id = addrId;
	}

	public String getYear_founded()
	{
		return year_founded;
	}

	public void setYear_founded(String yearFounded)
	{
		year_founded = yearFounded;
	}

	public String getRc_no()
	{
		return rc_no;
	}

	public void setRc_no(String rcNo)
	{
		rc_no = rcNo;
	}

	public int getOp_type_id()
	{
		return op_type_id;
	}

	public void setOp_type_id(int op_type_id)
	{
		this.op_type_id = op_type_id;
	}

	public String getOp_type_nm()
	{
		return op_type_nm;
	}

	public void setOp_type_nm(String op_type_nm)
	{
		this.op_type_nm = op_type_nm;
	}

}