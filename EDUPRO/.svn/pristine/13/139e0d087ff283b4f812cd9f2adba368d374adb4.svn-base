package com.dexter.bradawl.dto;

import java.util.Date;

public class StudentDamageFee implements java.io.Serializable
{
	private static final long serialVersionUID = 1L;
	
	private int student_damage_fee_id;
	private String damage_fee_desc;
	private double damage_fee_amount;
	private int student_id;
	private int term_id;
	private Date date_damaged;
	private int user_id;
	private Date crt_dt;
	
	private String student_nm;
	private double amt_paid;
	private double amt_bal;
	private Date date_paid;
	
	public StudentDamageFee()
	{}

	public int getTerm_id()
	{
		return term_id;
	}

	public void setTerm_id(int termId)
	{
		term_id = termId;
	}

	public int getStudent_damage_fee_id()
	{
		return student_damage_fee_id;
	}

	public void setStudent_damage_fee_id(int studentDamageFeeId)
	{
		student_damage_fee_id = studentDamageFeeId;
	}

	public String getDamage_fee_desc()
	{
		return damage_fee_desc;
	}

	public void setDamage_fee_desc(String damageFeeDesc)
	{
		damage_fee_desc = damageFeeDesc;
	}

	public double getDamage_fee_amount()
	{
		return damage_fee_amount;
	}

	public void setDamage_fee_amount(double damageFeeAmount)
	{
		damage_fee_amount = damageFeeAmount;
	}

	public int getStudent_id()
	{
		return student_id;
	}

	public void setStudent_id(int studentId)
	{
		student_id = studentId;
	}

	public Date getDate_damaged()
	{
		return date_damaged;
	}

	public void setDate_damaged(Date dateDamaged)
	{
		date_damaged = dateDamaged;
	}

	public int getUser_id()
	{
		return user_id;
	}

	public void setUser_id(int userId)
	{
		user_id = userId;
	}

	public Date getCrt_dt()
	{
		return crt_dt;
	}

	public void setCrt_dt(Date crtDt)
	{
		crt_dt = crtDt;
	}

	public String getStudent_nm()
	{
		return student_nm;
	}

	public void setStudent_nm(String student_nm)
	{
		this.student_nm = student_nm;
	}

	public double getAmt_paid()
	{
		return amt_paid;
	}

	public void setAmt_paid(double amt_paid)
	{
		this.amt_paid = amt_paid;
	}

	public double getAmt_bal()
	{
		return amt_bal;
	}

	public void setAmt_bal(double amt_bal)
	{
		this.amt_bal = amt_bal;
	}

	public Date getDate_paid() {
		return date_paid;
	}

	public void setDate_paid(Date date_paid) {
		this.date_paid = date_paid;
	}
	
}
